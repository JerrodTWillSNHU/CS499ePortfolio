/*****************************************************
*	   UPDATE FAMILY TABLE WITH AGE VALUES	     *
*****************************************************/

UPDATE Family
SET Age = 30
WHERE FirstName = 'Jerrod';

UPDATE Family
SET Age = 33
WHERE FirstName = 'Keri';

UPDATE Family
SET Age = 38
WHERE FirstName = 'Shena';

UPDATE Family
SET Age = 63
WHERE FirstName = 'Tom';

UPDATE Family
SET Age = 60
WHERE FirstName = 'Kathy';
