/*Updating Family table with Parents*/
UPDATE Family
SET Parents = 'Tom & Kathy'
WHERE FirstName = 'Jerrod';

UPDATE Family
SET Parents = 'Tom & Kathy'
WHERE FirstName = 'Keri';

