SET Parents = 'Tom & Kathy'
WHERE FirstName = 'Shena';

UPDATE Family
SET Parents = 'John & Irene'
WHERE FirstName = 'Tom';

UPDATE Family
SET Parents = 'Ed & Carol'
WHERE FirstName = 'Kathy';


